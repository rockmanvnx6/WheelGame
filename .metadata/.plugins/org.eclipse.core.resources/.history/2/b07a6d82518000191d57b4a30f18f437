package view;

import javax.swing.JOptionPane;


public class AddPlayerView extends JOptionPane {
	private MainFrame view;
	private static final long serialVersionUID = 1L;
	
	public AddPlayerView(MainFrame view) {
		this.view = view;
	}
	
	public void showMessageDialog(String message) {
		super.showMessageDialog(view, MESSAGE_PROPERTY);
	}
}
